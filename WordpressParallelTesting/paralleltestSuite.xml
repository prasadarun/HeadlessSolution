  <test name="chrome_Execution" >
    <packages>
      <package name="com.wordpress.xyz" />
   </packages>
 </test>
  <parameter name="myBrowser" value="Firefox" />
  <test name="Firefox_Execution" >
    <packages>
      <package name="com.wordpress.xyz" />
   </packages>
 </test>
</suite>






